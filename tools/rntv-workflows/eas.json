{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "android": {
        "resourceClass": "large",
        "image": "latest"
      },
      "ios": {
        "resourceClass": "large",
        "image": "latest"
      },
      "withoutCredentials": true,
      "distribution": "internal",
      "node": "22.14.0"
    },
    "rntester": {
      "extends": "base",
      "ios": {
        "config": "build-rntester-apple.yml"
      },
      "android": {
        "config": "build-rntester-android.yml"
      }
    }
  }
}
